Class A {
  public let foo: int;

  private def privateTest(): void {
    write("Inside privateTest");
    write(@foo);
    @foo = 80;
  }

  public def publicTest(): void {
    write("Inside publicTest");
    self.privateTest();
    write(@foo);
  }

  public A(a: int) {
    @foo = 70;
    self.privateTest();
  }
}


Class Main {
  public Main() {
    let a: A = new A();
    a.publicTest();
  }
}

